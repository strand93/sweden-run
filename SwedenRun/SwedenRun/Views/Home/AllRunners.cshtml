@{
  ViewBag.Title = "All Runners - Sweden Run";
}

@model IEnumerable<Runner>

<div class="container d-flex flex-column justify-content-center align-items-center">
  <h3 class="text-muted">All runners!</h3>

  <ul class="list-group">

    @foreach (Runner runner in Model)
    {
    <li class="list-group-item d-flex justify-content-between align-items-center">
      <img src="~/images/Runner.png" alt="Runner avatar" width="50" height="50">
      <div class="flex-column">
        <p class="text-muted p-0">Id:</p>
        <p class="p-0">@runner.RunnerId</p>
      </div>
      <div>
        TEST sadsadasd  asdas sad a
      </div>
    </li>
    }
  </ul>
</div>
